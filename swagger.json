  "info": {
    "title": "Task Management System API",
    "description": "A fully functional Task Management System backend built with **NestJS**,\n      providing secure authentication, role-based access control, and task and user management endpoints.\n      ",
    "version": "1.0.0",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "/api/v1"
    }
  ],
  "components": {
    "securitySchemes": {
      "accessToken": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "UserTaskDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "content": {
            "type": "object",
            "nullable": true
          },
          "status": {
            "type": "string",
            "enum": [
              "NOT_STARTED",
              "IN_PROGRESS",
              "PAUSED",
              "COMPLETED",
              "CANCELED"
            ],
            "example": "NOT_STARTED"
          },
          "assignedToUserId": {
            "type": "object",
            "nullable": true
          },
          "assignedAt": {
            "type": "object",
            "nullable": true
          },
          "completedAt": {
            "type": "object",
            "nullable": true
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "title",
          "content",
          "status",
          "assignedToUserId",
          "assignedAt",
          "completedAt",
          "createdAt",
          "updatedAt"
        ]
      },
      "UserProfileResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique user ID",
            "example": "c3f0f1f2-2d3a-4c1e-a9e6-1b1234567890"
          },
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "john@example.com"
          },
          "name": {
            "type": "string",
            "description": "Full name of the user",
            "example": "John Doe"
          },
          "role": {
            "type": "string",
            "description": "Role of the user",
            "example": "USER",
            "enum": [
              "ADMIN",
              "USER"
            ]
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "description": "Account creation timestamp",
            "example": "2025-01-25T10:15:30.000Z"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "description": "Last updated timestamp",
            "example": "2025-01-26T09:20:15.000Z"
          },
          "tasks": {
            "description": "Tasks assigned to the user",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserTaskDto"
            }
          }
        },
        "required": [
          "id",
          "email",
          "name",
          "role",
          "createdAt",
          "updatedAt",
          "tasks"
        ]
      },
      "DeleteAccountResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "required": [
          "message"
        ]
      },
      "UserSummaryDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "c3f0f1f2-2d3a-4c1e-a9e6-1b1234567890"
          },
          "email": {
            "type": "string",
            "example": "john@example.com"
          },
          "name": {
            "type": "string",
            "example": "John Doe"
          },
          "role": {
            "type": "string",
            "enum": [
              "ADMIN",
              "USER"
            ],
            "example": "USER"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-01-25T10:15:30.000Z"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-01-26T09:20:15.000Z"
          }
        },
        "required": [
          "id",
          "email",
          "name",
          "role",
          "createdAt",
          "updatedAt"
        ]
      },
      "AllUsersResponseDto": {
        "type": "object",
        "properties": {
          "users": {
            "description": "List of all registered users",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserSummaryDto"
            }
          }
        },
        "required": [
          "users"
        ]
      },
      "UpdateRoleDto": {
        "type": "object",
        "properties": {
          "role": {
            "type": "string",
            "description": "Role to assign to the user",
            "example": "ADMIN",
            "enum": [
              "ADMIN",
              "USER"
            ]
          }
        },
        "required": [
          "role"
        ]
      },
      "CreateTaskDto": {
        "type": "object",
        "properties": {}
      },
      "UpdateAssignedTaskStatusDto": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Status of Tasks",
            "example": "IN_PROGRESS",
            "enum": [
              "NOT_STARTED",
              "IN_PROGRESS",
              "PAUSED",
              "COMPLETED",
              "CANCELED"
            ]
          }
        },
        "required": [
          "status"
        ]
      },
      "UpdateTaskDto": {
        "type": "object",
        "properties": {}
      },
      "TaskResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "3a95cdae-9f35-4c98-8ff1-13b9b72123de"
          },
          "title": {
            "type": "string",
            "example": "Fix login bug"
          },
          "content": {
            "type": "object",
            "example": "There is a redirect issue after login.",
            "nullable": true
          },
          "status": {
            "type": "string",
            "enum": [
              "NOT_STARTED",
              "IN_PROGRESS",
              "PAUSED",
              "COMPLETED",
              "CANCELED"
            ],
